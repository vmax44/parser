@model IEnumerable<ParserSite.Order>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OrderNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClientName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClientCar)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrderDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DTPDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ParsedDatas)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrderNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClientName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClientCar)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OrderDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DTPDate)
            </td>
            <td></td>
            <td>
                @Html.ActionLink("Edit", "Edit", "Orders", new { id = item.Id },
                    new { @class = "viewDialog", data_dialog_title = "Редактирование заказа" }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", "Orders", new { id = item.Id },
                    new { @class = "viewDialog", data_dialog_title = "Удаление заказа" })
            </td>
        </tr>
    }

</table>

<p> @Html.ActionLink("Add New", "Create", "Orders", new { }, new { @class = "viewDialog", data_dialog_title = "Создание заказа" })</p>
<script>
    $(document).ready(dialogrun());
</script>